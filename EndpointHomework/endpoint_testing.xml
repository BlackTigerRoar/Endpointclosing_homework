<?xml version="1.0" encoding="UTF-8"?>
<suite name="Suite" parallel="false">
    <listeners>
        <listener class-name="listeners.TestListener" />
    </listeners>
    <test name="iOS_Test">
        <parameter name="platformName" value="iOS" />
        <parameter name="platformVersion" value="12.1" />
        <parameter name="deviceName" value="iPhone 8" />

        <classes>
            <class name="tests.WikiAppTestsIOS" />
        </classes>
    </test>

    <test name="Andorid_Test">
        <parameter name="platformName" value="Android" />
        <parameter name="platformVersion" value="10" />
        <parameter name="deviceName" value="Test Device" />

        <classes>
            <class name="tests.WikiAppTests" />
        </classes>
    </test>
</suite>